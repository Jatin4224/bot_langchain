{
  "name": "langchain",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "concurrently \"bun --watch index.ts\" \"bun --cwd client dev\"",
    "server": "bun --watch index.ts",
    "client": "bun --cwd client dev",
    "build": "cd client && bun run build"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "concurrently": "^9.2.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "workspaces": [
    "packages/*",
    "client"
  ]
}
